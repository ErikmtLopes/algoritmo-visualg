algoritmo "semnome"
// Função :
// Autor :
// Data : 13/09/2018
// Seção de Declarações 
var
n1, n2, n3:inteiro
inicio
      escreva ("Digite um número: ")
      leia (n1)
      escreva ("Digite um outro número: ")
      leia (n2)
      n3 <- (n1+n2)
      se (n3>20) entao
         escreva (n3+8)
         senao
              escreva (n3-5)
      fimse
fimalgoritmo
